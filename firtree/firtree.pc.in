prefix=@CMAKE_INSTALL_PREFIX@
libdir=${prefix}/lib
includedir=@FIRTREE_HEADER_INSTALL_DEST@

Name: firtree
Description: The Firtree image processing library.
Version: @FIRTREE_VERSION_MAJOR@.@FIRTREE_VERSION_MINOR@.@FIRTREE_VERSION_PATCH@
Requires: cairo clutter-0.8 gdk-pixbuf-2.0
Libs: -L${libdir} -lfirtree-core-@FIRTREE_VERSION_MAJOR@.@FIRTREE_VERSION_MINOR@ -lfirtree-engine-cpu-@FIRTREE_VERSION_MAJOR@.@FIRTREE_VERSION_MINOR@
Cflags: -I${includedir} 
