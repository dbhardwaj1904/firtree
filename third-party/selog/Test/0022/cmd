while :
do
	sec=$(( $(date +%S) + 1))
	case $sec in
	?)	sec=0$sec
		break
		;;
	6?)	continue
		;;
	??)	break
		;;
	*)	exit 1
		;;
	esac
done
export TZ=ABC$(TZ=UTC0 date +%H:%M:$sec)
day1=$(date +%Y-%m-%d)
../test-harness -s80 +log_rotate-log_tz@/WORK/selall
day2=$(date +%Y-%m-%d)
sed "s/$day1 23:59:59/YYYY-YY-YY YY:YY:YY/" </WORK/selall.$day1 >/WORK/selall
sed "s/$day2 00:00:00/YYYY-YY-YY YY:YY:YY/" </WORK/selall.$day2 >>/WORK/selall
rm /WORK/selall.$day1 /WORK/selall.$day2
